{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/Backend/index.js",
            "runtimeExecutable": "nodemon",
            "restart": true,
            "runtimeArgs": [
                "--inspect"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}